{
	"name": "ChangePageColor",
	"description": "A browser action with a popup that changes the page color.",
	"version": "1.0",
	"permissions": [
		"tabs", "http://*/*", "https://*/*"
	],
	"browser_action": {
		"default_title": "Set the page's color",
		"dafault_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"manifest_version": 2
}
